body
  margin 0
  padding 0
  background #efefef
  font-size 14px
ul
  list-style none
  margin 0
  padding 0

header
  width 100%
  height 320px
  overflow-x hidden
  position relative
  .title
    position absolute
    top 25px
    left 0
    right 0
    padding 0 10px
    height 25px
    line-height 25px
    color #ffffff
    .city
      display block
      float left
      width 38%
      &:after
        content ""
        width 10px
        height 7px
        background url("../../img/nav_icon_down@2x.png") no-repeat center
        background-size 10px 7px
        display block
        position absolute
        top 9px
        left 45px
    .titles
      display block
      float left
      font-size 20px
  .search
    width 94%
    height 35px
    line-height 35px
    position absolute
    top 65px
    left 3%
    text-align center
    background #ffffff
    border-radius 5px
    color #b8b8b8
    font-size 12px
    &:after
      content ""
      width 14px
      height 14px
      display block
      background url("../../img/nav_icon_search@2x.png") no-repeat center
      background-size 14px 14px
      position absolute
      top 10px
      left 28%
  .slider
    height 320px
    width 400%
    transition all .5s
    transform translateX(0)
    li
      float left
      width 25%
      img
        width 100%
        height 320px
  .index
    width 100px
    height 25px
    position absolute
    bottom 10px
    left 50%
    margin-left -50px
    li
      width 25px
      height 25px
      float left
      background url("../../img/banner_icon_point_nor@2x.png") no-repeat center
      &.active
        background url("../../img/banner_icon_point_press@2x.png") no-repeat center
article
  margin-top 10px
  ul
    height 45px
    background #ffffff
    border-bottom 1px solid #d7d8dd
    li
      float left
      margin-top 5px
      height 30px
      line-height 30px
      width 21%
      text-align center
      position relative
      border-right 1px solid #d7d8dd
      color #646464
      &:after
        content ""
        width 10px
        height 6px
        background url("../../img/content_icon02_nor@2x.png") no-repeat center
        background-size 10px 6px
        display block
        position absolute
        bottom -3px
        left 50%
        margin-left -5px
      &:last-child
        width 14%
        border-right none
        background url("../../img/content_icon01_nor@2x.png") no-repeat center
        background-size 19px 19px
        &:after
          content ""
          display none
        &.up
          background url("../../img/content_icon01_litre@2x.png") no-repeat center
          background-size 19px 19px
        &.down
          background url("../../img/content_icon01_down@2x.png") no-repeat center
          background-size 19px 19px
.list
  height 115px
  padding 10px
  background #ffffff
  margin-top 10px
  position relative
  .icon-back
    display block
    width 34%
    margin-right 6%
    height 110px
    float left
    background url("../../img/content_img_list@2x.png") no-repeat center
    background-size 130px 110px
  .info
    float left
    width 53%
    height 100px
    line-height 25px
    font-size 12px
    color #646464
    .title
      color #323232
      display block
      font-size 16px
      font-weight bold
      margin-top 0
      .icon-lo
        display block
        width 15px
        height 15px
        background url("../../img/collect_icon_nor@2x.png") no-repeat center
        background-size 15px 15px
        position absolute
        right 6%
        top 15px
        &.active
          background url("../../img/collect_icon_press@2x.png") no-repeat center
          background-size 15px 15px
    .add
      font-size 14px
    button
      color tomato
      background #ffffff
      border 1px solid tomato
      border-radius 5px
    .info-btn
      display inline-block
      .price
        color tomato
        font-size 16px
      .date
        display inline-block
        position absolute
        right 5px
  &:last-child
    margin-bottom 50px
.footer
	position fixed
	height 50px
	width 100%
	bottom 0
	overflow hidden
	background #fff
	padding 0
	margin 0
	.footer_li
		float left
		list-style none
		width 20%
		height 20px
		text-align center
		padding-top 30px
		font-size 12px
		color #b4b4b4
		&:nth-child(1)
			background url(../../img/bottom_press_icon04@2x.png) no-repeat center top 7px #fff
			background-size 22px 21px
			color #ea5404
		&:nth-child(2)
			background url(../../img/bottom_nor_icon03@2x.png) no-repeat center top 7px #fff
			background-size 22px 21px
		&:nth-child(3)
			background url(../../img/bottom_icon05@2x.png) no-repeat center top 7px #fff
			background-size 44px 38px
		&:nth-child(4)
			background url(../../img/bottom_nor_icon02@2x.png) no-repeat center top 7px #fff
			background-size 22px 21px
		&:nth-child(5)
			background url(../../img/bottom_nor_icon01@2x.png) no-repeat center top 7px #fff
			background-size 22px 21px
.publish
  position fixed
  top 0
  bottom 0
  width 100%
  z-index 10
  display none
  background rgba(0,0,0,0.9)
  .con
    width 68%
    position fixed
    left 16%
    bottom 110px
    height 120px
    .left,.right
      width 95px
      height 95px
      border-radius 50%
      float left
      background url("../../img/publish_btn02@2x.png") no-repeat center
      background-size 95px 95px
      position relative
      span
        display block
        width 95px
        text-align center
        position absolute
        bottom -25px
        color: #ffffff
      &.right
        background url("../../img/publish_btn01@2x.png") no-repeat center
        background-size 95px 95px
        float right
  .bottom
    height 60px
    background url("../../img/publish_close_icon@2x.png") no-repeat center #242221
    background-size 16px 16px
    position fixed
    bottom 0
    width 100%

.icon-top
  width 47px
  height 47px
  display block
  background url("../../img/content_btn_back@2x.png") no-repeat center
  background-size 47px 47px
  position fixed
  right 12%
  bottom 90px
.search-box,.city-box
  position fixed
  top 0
  bottom 0
  width 100%
  background #ffffff
  display none
  .head
    height 50px
    line-height 50px
    &:after
      content ""
      width 14px
      height 14px
      display block
      background url("../../img/nav_icon_search@2x.png") no-repeat center
      background-size 14px 14px
      position absolute
      top 20px
      left 6%
    .search1
      width 80%
      height 35px
      line-height 35px
      border none
      outline none
      position absolute
      top 10px
      left 3%
      text-align left
      text-indent 3em
      background #efefef
      border-radius 5px
      color #ea5404
      font-size 12px

    .cancel
      display block
      color #ea5404
      position absolute
      right 3%
      top 0
  .menu
    height 35px
    line-height 35px
    background #efefef
    color #646464
    padding 0 6% 0 9%
    .clean
      float right
  ul
    position absolute
    top 85px
    bottom 0
    left 0
    right 0
    padding-left 9%
    font-size 18px
    font-weight 500
    li
      height 35px
      line-height 35px
      border-bottom 1px solid #e1e1e1
.city-box
  z-index
  .head
    font-weight bold
    text-align center
    padding 0 5%
    font-size 18px
    border-bottom 1px solid #d6d7dc
    &:after
      content ""
      display none
    .close
      color #ea5404
      font-weight 400
      float left
  ul
    top 50px
    li:last-child
      border-bottom none

.query-box
  position fixed
  top 0
  bottom 68px
  width 100%
  background rgba(0,0,0,0.5)
  display none
  .top
    height 50px
    line-height 50px
    padding 0 3%
    color #ffffff
    background #000000
    &:after
      content ""
      width 14px
      height 14px
      display block
      background url("../../img/nav_icon_search@2x.png") no-repeat center
      background-size 14px 14px
      position absolute
      top 20px
      left 30%
    .city
      display block
      float left
      width 38%
      &:after
        content ""
        width 10px
        height 7px
        background url("../../img/nav_icon_down@2x.png") no-repeat center
        background-size 10px 7px
        display block
        position absolute
        top 22px
        left 45px
    .search1
      width 60%
      height 30px
      line-height 30px
      border none
      outline none
      position absolute
      top 10px
      left 20%
      text-align center
      background #efefef
      border-radius 5px
      color #ea5404
      font-size 12px
  .query-title
    height 45px
    background #ffffff
    border-bottom 1px solid #d7d8dd
    li
      color #646464
      float left
      margin-top 10px
      height 30px
      line-height 30px
      width 21%
      text-align center
      position relative
      border-right 1px solid #d7d8dd
      &:after
        content ""
        width 10px
        height 6px
        background url("../../img/content_icon02_nor@2x.png") no-repeat center
        background-size 10px 6px
        display block
        position absolute
        bottom -2px
        left 50%
        margin-left -5px
      &.active
        color #ea5404
        &:after
          content ""
          width 10px
          height 6px
          background url("../../img/content_icon02_press@2x.png") no-repeat center
          background-size 10px 6px
          display block
          position absolute
          bottom -2px
          left 50%
          margin-left -5px
      &:last-child
        width 14%
        border-right none
        background url("../../img/content_icon01_nor@2x.png") no-repeat center
        background-size 19px 19px
        &:after
          content ""
          display none
        &.up
          background url("../../img/content_icon01_litre@2x.png") no-repeat center
          background-size 19px 19px
        &.down
          background url("../../img/content_icon01_down@2x.png") no-repeat center
          background-size 19px 19px
  .content
    height 425px
    background #efefef
    position relative
    li.active
      background #ffffff
    li.active2
      color #ea5404
    .region,.many
      display none
      ul
        width 50%
        height 100%
        background #efefef
        &.left li:hover
          background #ffffff
        &.right
          position absolute
          background #ffffff
          display none
          right 0
          top 0
          &.actives
            display block
          li
            border-bottom 1px solid #d7d8dd
        li
          height 30px
          line-height 30px
          padding-left 6%
    .area,.price
      display none
      li
        height 30px
        width 100%
        line-height 30px
        text-align center
        &:hover
          background #ffffff
        &:last-child
          color #b4b4b4
          padding 5px
          &:hover
            background #efefef
        input
          width 70px
          height 30px
          border none
          border-radius 5px
        button
          width 55px
          height 30px
          border none
          border-radius 5px
          background #ffffff
          margin-left 10px
.cover
	display none
	position absolute
	left 0
	right 0
	top 0
	bottom 0
	background rgba(0,0,0,0.9)
	z-index 9
	.control
		position absolute
		bottom 107px
		width 67.2%
		margin 0 16.4%
		overflow hidden
		span
			display block
			width 100%
			text-align center
			color #ffffff
			line-height 24px
		.circle_left
			padding-top 94px
			float left
			width 94px
			background url(../../img/publish_btn02@2x.png) no-repeat left 0 top 0
			background-size 94px 94px

		.circle_right
			padding-top 94px
			float right
			width 94px
			background url(../../img/publish_btn01@2x.png) no-repeat left 0 top 0
			background-size 94px 94px
	.close
		position absolute
		bottom 0
		left 0
		right 0
		z-index 2
		height 48px
		background url(../../img/publish_close_icon@2x.png) no-repeat center #242221
		background-size 14px 14px
	&.active
		display block






