body
	padding 0
	margin 0
	background #efeff4
.header
	height 40px
	background-color #fff
	line-height 40px
	.icon
		position absolute
		top 4px
		left 10px
		display inline-block
		width 35px
		height 35px
		background url(../../img/nav_icon_back@2x.png) no-repeat center
		background-size 10px 18px
	.header_title
		display inline-block
		width 100%
		text-align center
		border-bottom 1px solid #d6d7dc
.logo
	.logo1
		width 74px
		height 74px
		background url(../../img/content_img_logo@2x.png) no-repeat center
		background-size 74px 74px
		margin 0 auto
		margin-top 45px
	.logo2
		text-align center
		font-size 12px
		line-height 22px
		margin-top 10px
		margin-bottom 65px
		color #848486
.setting_body
	margin 0
	padding 0
	li
		height 50px
		background #fff
		list-style none
		padding-left 10px
		line-height 50px
		position relative
		.right_icon
			position absolute
			width 10px
			height 18px
			background url(../../img/login_right.png) no-repeat center
			background-size 10px 18px
			right 13px
			top 50%
			margin-top -9px
		.phone_num
			color #B4B4B4
			font-size 14px
			display block
			position absolute
			right 31px
			top 0
		&:nth-child(2)
			margin-top 11px
.footer
	display block
	width 100%
	text-align center
	position absolute
	bottom 37px
	color #a4a4a7
.gob
	position absolute
	left 0
	top 0
	right 0
	bottom 0
	Background:rgba(167,167,170,0.5)
	.box_confirm
		height 108px
		width 73%
		background #fff
		border-radius 10px
		margin 278px 13.5% 0 13.5%
		.box_top
			height 63px
			width 100%
			border-bottom 1px solid #d6d7dc
			text-align center
			line-height 63px
			color #323232
			font-size 12px
		.box_bottom
			width 100%
			line-height 44px
			text-align center
			font-size 14px
			color #ea5404
			overflow hidden
			.box_bottom_left
				float left
				width 49.8%
				text-align center
				color #646464
				border-right 1px solid #d6d7dc
			.box_bottom_right
				float left
				width 49.8%
				text-align center
				color #ea5404